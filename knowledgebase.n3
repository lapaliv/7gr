@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Base classes
ex:Sector rdf:type rdfs:Class .
ex:Device rdf:type rdfs:Class .
ex:Sensor rdf:type rdfs:Device .
ex:Conditioner rdf:type rdfs:Device .
ex:Humidifier rdf:type rdfs:Device .
ex:Dehumidifier rdf:type rdfs:Device .
ex:Camera rdf:type rdfs:Device .
ex:TemperatureSensor rdf:type rdfs:Sensor .
ex:HumiditySensor rdf:type rdfs:Sensor .
ex:ControlPanel rdf:type rdfs:Class .
ex:Rule rdf:type rdfs:Class .

# Properties
ex:hasConditioner a rdf:Property ;
    rdfs:range ex:Conditioner .
ex:hasHumidifier a rdf:Property ;
    rdfs:range ex:Humidifier .
ex:hasDehumidifier a rdf:Property ;
    rdfs:range ex:Dehumidifier .
ex:hasCamera a rdf:Property ;
    rdfs:range ex:Camera .
ex:hasTemperatureSensor a rdf:Property ;
    rdfs:range ex:TemperatureSensor .
ex:hasHumiditySensor a rdf:Property ;
    rdfs:range ex:HumiditySensor .

# Instances
ex:Sector1 rdf:type ex:Sector ;
    ex:label "Sector 1" .
ex:Sector2 rdf:type ex:Sector ;
    ex:label "Sector 2" .
ex:Conditioner1 rdf:type ex:Conditioner .
ex:Conditioner2 rdf:type ex:Conditioner .
ex:Conditioner3 rdf:type ex:Conditioner .
ex:Humidifier1 rdf:type ex:Humidifier .
ex:Humidifier2 rdf:type ex:Humidifier .
ex:Humidifier3 rdf:type ex:Humidifier .
ex:Dehumidifier1 rdf:type ex:Dehumidifier .
ex:Dehumidifier2 rdf:type ex:Dehumidifier .
ex:Dehumidifier3 rdf:type ex:Dehumidifier .
ex:Camera1 rdf:type ex:Camera .
ex:Camera2 rdf:type ex:Camera .
ex:Camera3 rdf:type ex:Camera .
ex:TemperatureSensor1 rdf:type ex:TemperatureSensor .
ex:TemperatureSensor2 rdf:type ex:TemperatureSensor .
ex:TemperatureSensor3 rdf:type ex:TemperatureSensor .
ex:HumiditySensor1 rdf:type ex:HumiditySensor .
ex:HumiditySensor2 rdf:type ex:HumiditySensor .
ex:HumiditySensor3 rdf:type ex:HumiditySensor .

# Relations
ex:Sector1 ex:hasConditioner ex:Conditioner1 .
ex:Sector1 ex:hasConditioner ex:Conditioner2 .
ex:Sector1 ex:hasHumidifier ex:Humidifier1 .
ex:Sector1 ex:hasHumidifier ex:Humidifier2 .
ex:Sector1 ex:hasDehumidifier ex:Dehumidifier1 .
ex:Sector1 ex:hasDehumidifier ex:Dehumidifier2 .
ex:Sector1 ex:hasCamera ex:Camera1 .
ex:Sector1 ex:hasCamera ex:Camera2 .
ex:Sector1 ex:hasTemperatureSensor ex:TemperatureSensor1 .
ex:Sector1 ex:hasTemperatureSensor ex:TemperatureSensor2 .
ex:Sector1 ex:hasHumiditySensor ex:HumiditySensor1 .
ex:Sector1 ex:hasHumiditySensor ex:HumiditySensor2 .

ex:Sector2 ex:hasConditioner ex:Conditioner3 .
ex:Sector2 ex:hasHumidifier ex:Humidifier3 .
ex:Sector2 ex:hasDehumidifier ex:Dehumidifier1 .
ex:Sector2 ex:hasCamera ex:Camera3 .
ex:Sector2 ex:hasTemperatureSensor ex:TemperatureSensor1 .
ex:Sector2 ex:hasHumiditySensor ex:HumiditySensor3 .

